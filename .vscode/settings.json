{
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "[json][yaml][yml][md]": {
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.autoIndent": "full",
    "editor.trimAutoWhitespace": true
  },
  "[makefile]": {
    "editor.tabSize": 4,
    "editor.insertSpaces": false,
    "editor.detectIndentation": false
  },
  "ansible.python.interpreterPath": "/bin/python3"
}
